<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kcttwarehousemobilesystem">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_app_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_app_logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.KCTTWarehouseMobileSystem">
        <activity android:name=".ForgetPassword"></activity>
        <activity android:name=".Reset" />
        <activity android:name=".Register" />
        <activity android:name=".Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".MaterialDetailsRetrieve"
            android:parentActivityName=".RetrieveFromRackScanner" />
        <activity
            android:name=".RetrieveFromRackScanner"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".WarehouseMap"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".PlaceToRackScanner"
            android:parentActivityName=".MaterialDetails" />
        <activity
            android:name=".MaterialDetails"
            android:parentActivityName=".ReceiveMaterialsScanner" />
        <activity
            android:name=".ReceiveMaterialsScanner"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MainMaterialList"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MainMaterialAct"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Report"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".MainActivity">

        </activity>
    </application>

</manifest>